<?xml version="1.0" encoding="UTF-8"?>
<bundle>
  <UISEOForm>
    <label>
      <title>Titre: </title>
      <description>Description: </description>
      <keywords>Mots clés: </keywords>
      <language>Langue: </language>
      <robots>Robots: </robots>
      <sitemap>Sitemap: </sitemap>
      <sitemapvisible>Visible dans le sitemap</sitemapvisible>
      <priority>Priorité: </priority>
      <frequency>Fréquence: </frequency>
      <select-language>Select a language</select-language>
      <option>
        <Always>Toujours</Always>
        <Hourly>Toutes les heures</Hourly>
        <Daily>Journalier</Daily>
        <Weekly>Hebdomadaire</Weekly>
        <Monthly>Mensuel</Monthly>
        <Yearly>Annuel</Yearly>
        <Never>Jamais</Never>
      </option>      
    </label>
    <tip>
        <priority>Valeurs valides de 0.0 à 1.0.</priority>
    </tip>  
    <action>
      <Save>Sauvegarder</Save>
      <Cancel>Annuler</Cancel>
    </action>
    <msg>
      <repository-exception>Une erreure inattendue est apparue! Sauvegarde impossible des métadonnées.</repository-exception>
      <confirm-delete>Êtes-vous sûr de vouloir supprimer les données SEO?</confirm-delete>   
    </msg>
    <title>
    	<seo-delete>Supprimer</seo-delete>
    </title>
    <help> 
		  <priority>Valid values range from 0.0 to 1.0.</priority>
		  <description>Help searchers to let them know what the following page has a link with what they are looking for.</description>
		  <keywords>Most important terms in the page separated by a comma.</keywords>		  
	  </help>	
  </UISEOForm>
  <UISEOPopupWindow>
    <title>
      <UISEOForm>SEO Management</UISEOForm>
    </title>    
  </UISEOPopupWindow>
  <UISEOToolbarForm>
    <title>
      <seo>SEO Management</seo>
    </title>
    <label>
  	  <seo>SEO</seo>
  	</label>
  </UISEOToolbarForm>
  <FloatNumberValidator>
    <msg>
      <Invalid-number>Le champ '{0}' est un nombre invalide (float).</Invalid-number>
    </msg>
  </FloatNumberValidator> 
  
</bundle>