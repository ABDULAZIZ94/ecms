<?xml version="1.0" encoding="UTF-8"?>
<bundle>
  <UISEOForm>
    <label>
	  <title>Tiêu đề: </title>
      <description>Mô tả: </description>
      <keywords>Từ khóa: </keywords>
      <language>Ngôn ngữ: </language>
      <robots>Robots: </robots>
      <sitemap>Sitemap: </sitemap>
      <sitemapvisible>Hiển thị trên sitemap</sitemapvisible>
      <priority>Độ ưu tiên: </priority>
      <frequency>Tần suất: </frequency>
      <select-language>Select a language</select-language>
      <option>
        <Always>Luôn luôn</Always>
        <Hourly>Hàng giờ</Hourly>
        <Daily>Hàng ngày</Daily>
        <Weekly>Hàng tuần</Weekly>
        <Monthly>Hàng tháng</Monthly>
        <Yearly>Hàng năm</Yearly>
        <Never>Không bao giờ</Never>        
      </option>
    </label>
	<tip>
		<priority>Giá trị hợp lệ từ 0.0 đến 1.0</priority>
	</tip>  
    <action>
      <Save>Cập nhật</Save>
      <Cancel>Hủy bỏ</Cancel>
    </action>
    <msg>
      <repository-exception>Có lỗi xảy ra trên hệ thộng. Không thể cập nhật được dữ liệu.</repository-exception>
      <confirm-delete>Are you sure you want to delete these SEO data?</confirm-delete>	
    </msg>
    <title>
    	<seo-delete>Delete</seo-delete>
    </title>
	  <help> 
		  <priority>Valid values range from 0.0 to 1.0.</priority>
		  <description>Help searchers to let them know what the following page has a link with what they are looking for.</description>
		  <keywords>Most important terms in the page separated by a comma.</keywords>		  
	  </help>	
  </UISEOForm>
  <UISEOPopupWindow>
  	<title>
      <UISEOForm>Quản lý SEO</UISEOForm>
  	</title>
  </UISEOPopupWindow>
  <UISEOToolbarForm>
  	<title>
      <seo>Quản lý SEO</seo>
  	</title>
  	<label>
  	  <seo>SEO</seo>
  	</label>
  </UISEOToolbarForm>
  <FloatNumberValidator>
    <msg>
      <Invalid-number>Trường {0} chỉ nhận giá trị số kiểu float.</Invalid-number>
    </msg>
  </FloatNumberValidator>  
</bundle>
