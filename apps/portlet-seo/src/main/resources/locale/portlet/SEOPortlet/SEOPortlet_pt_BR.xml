<?xml version="1.0" encoding="UTF-8"?>
<bundle>
  <UISEOForm>
    <label>   
      <description>Descrição: </description>
      <keywords>Palavras chave: </keywords>
      <language>Idioma: </language>
      <robots>Robos: </robots>
      <sitemap>Mapa do site: </sitemap>
      <sitemapvisible>Vísivel no mapa do site</sitemapvisible>
      <isInherited>Herdado da página pai</isInherited>
      <priority>Prioridade: </priority>
      <frequency>Frequência: </frequency>
      <select-language>Select a language</select-language> 
      <option>
        <Always>Sempre</Always>
        <Hourly>Hora</Hourly>
        <Daily>Diária</Daily>
        <Weekly>Semanal</Weekly>
        <Monthly>Mensal</Monthly>
        <Yearly>Anual</Yearly>
        <Never>Nunca</Never>
      </option> 
    </label>
    <tip>
        <priority>Valores válidos de 0.0 a 1.0</priority>
    </tip>        
    <action>
      <Save>Salvar</Save>
      <Cancel>Cancelar</Cancel>
    </action>
    <msg>
      <repository-exception>Ocorreu um erro inesperado enquanto salvava a página metadata.</repository-exception>
      <confirm-delete>Are you sure you want to delete these SEO data?</confirm-delete>   
    </msg>
    <title>
    	<seo-delete>Delete</seo-delete>
    </title>
	  <help> 
		  <priority>Valid values range from 0.0 to 1.0.</priority>
		  <description>Help searchers to let them know what the following page has a link with what they are looking for.</description>
		  <keywords>Most important terms in the page separated by a comma.</keywords>		  
	  </help>	   
  </UISEOForm>
  <UISEOPopupWindow>
    <title>
      <UISEOForm>Administração SEO</UISEOForm>
    </title>
  </UISEOPopupWindow>    
  <UISEOToolbarForm>
    <title>
      <seo>Administração SEO</seo>
    </title>
  </UISEOToolbarForm>  
  <FloatNumberValidator>
    <msg>
      <Invalid-number>O valor do campo prioridade deve ser no formato numérico. Os valores válidos são de 0.0 a 1.0.</Invalid-number>
    </msg>
  </FloatNumberValidator>  
  
</bundle>