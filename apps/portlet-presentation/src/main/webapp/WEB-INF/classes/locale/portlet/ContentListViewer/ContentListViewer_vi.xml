<?xml version="1.0" encoding="UTF-8"?>
<bundle>
  <UICLVContainer>
    <title>
      <QuickEdit>Sửa nhanh</QuickEdit>
    </title>
    <msg>
      <non-contents>Không có nội dung nào</non-contents>
      <item-not-found>Nội dung này không tồn tại</item-not-found>
      <no-permission>Bạn không có quyền xem nội dung này</no-permission>	
    </msg>
  </UICLVContainer>

  <UIViewerManagementPopupWindow>
    <title>
      <UICLVConfig>Cấu hình cho Content list viewer</UICLVConfig>
    </title>
  </UIViewerManagementPopupWindow>

  <UICLVConfig>
    <label>
      <Title>Cấu hình cho Content list viewer</Title>
      <ContentSelectionBlock>Chọn nội dung</ContentSelectionBlock>
      <UICLVConfigDisplayModeFormRadioBoxInput>Chế độ</UICLVConfigDisplayModeFormRadioBoxInput>
      <UICLVConfigItemPathFormInputSet>Đường dẫn</UICLVConfigItemPathFormInputSet>
      <UICLVConfigOrderByFormSelectBox>Sắp xếp theo</UICLVConfigOrderByFormSelectBox>
      <option>
        <exo:title>Tiêu đề</exo:title>
        <exo:dateCreated>Ngày tạo</exo:dateCreated>
        <exo:dateModified>Ngày sửa</exo:dateModified>
        <publication:liveDate>Ngày xuất bản</publication:liveDate>
      </option>
      <DisplaySettingBlock>Lựa chọn hiển thị</DisplaySettingBlock>
      <UICLVConfigHeaderFormStringInput>Tiêu đề portlet</UICLVConfigHeaderFormStringInput>
      <UICLVConfigDisplayTemplateFormSelectBox>Mẫu hiển thị</UICLVConfigDisplayTemplateFormSelectBox>
      <UICLVConfigPaginatorTemplateFormSelectBox>Mẫu phân trang</UICLVConfigPaginatorTemplateFormSelectBox>
      <UICLVConfigTargetPageFormInputSet>Trang đích</UICLVConfigTargetPageFormInputSet>
      <UICLVConfigTargetPageFormStringInput></UICLVConfigTargetPageFormStringInput>
      <UICLVConfigItemsPerPageFormStringInput>Số lượng mỗi trang</UICLVConfigItemsPerPageFormStringInput>
      <UICLVConfigShowIllustrationFormCheckboxInput>Hiển thị hình minh hoạ</UICLVConfigShowIllustrationFormCheckboxInput>
      <UICLVConfigShowSummaryFormCheckboxInput>Hiển thị tóm tắt</UICLVConfigShowSummaryFormCheckboxInput>
      <UICLVConfigShowHeaderFormCheckboxInput>Hiển thị tiêu đề portlet</UICLVConfigShowHeaderFormCheckboxInput>
      <UICLVConfigShowRefreshFormCheckboxInput>Hiển thị nút làm mới</UICLVConfigShowRefreshFormCheckboxInput>
      <UICLVConfigShowTitleFormCheckboxInput>Hiển thị tiêu đề nội dung</UICLVConfigShowTitleFormCheckboxInput>
      <UICLVConfigShowDateCreatedFormCheckboxInput>Hiển thị ngày tạo</UICLVConfigShowDateCreatedFormCheckboxInput>
      <UICLVConfigShowLinkFormCheckboxInput>Hiển thị liên kết</UICLVConfigShowLinkFormCheckboxInput>
      <UICLVConfigShowReadmoreFormCheckboxInput>Hiển thị Đọc thêm</UICLVConfigShowReadmoreFormCheckboxInput>
    </label>
    <action>
      <Save>Lưu</Save>
      <Cancel>Hủy</Cancel>
    </action>
    <msg>
      <not-valid-path>Đường dẫn không đúng</not-valid-path>
      <saving-success>Cấu hình cho portlet đã được lưu lại</saving-success>
    </msg>
  </UICLVConfig>

  <UICLVConfigDisplayModeFormRadioBoxInput>
    <label>
      <AutoViewerMode>Tự động</AutoViewerMode>
      <ManualViewerMode>Tùy chọn</ManualViewerMode>
    </label>
  </UICLVConfigDisplayModeFormRadioBoxInput>

  <UICLVConfigItemPathFormInputSet>
    <title>
      <SelectFolderPath>Chọn thư mục</SelectFolderPath>
    </title>
  </UICLVConfigItemPathFormInputSet>

  <FolderPathSelectorPopupWindow>
    <title>
      <UIContentSelectorFolder>Duyệt thư mục</UIContentSelectorFolder>
    </title>
  </FolderPathSelectorPopupWindow>

  <UICLVConfigOrderTypeFormRadioBoxInput>
    <label>
      <OrderDesc>Giảm dần</OrderDesc>
      <OrderAsc>Tăng dần</OrderAsc>
    </label>
  </UICLVConfigOrderTypeFormRadioBoxInput>

  <UICLVConfigTargetPageFormInputSet>
    <title>
      <SelectBasePath>Chọn trang đích</SelectBasePath>
    </title>
  </UICLVConfigTargetPageFormInputSet>

  <UICLVBasePathPopupWindow>
