<?xml version="1.0" encoding="UTF-8"?>
<bundle>
 <UIPresentationContainer>
  <title>
   <QuickEdit>Quick edit</QuickEdit>
   <print>Print this page</print>
  </title>
  <label>
   <Draft>Draft</Draft>
   <print>Print</print>
   <Preferences>Preferences</Preferences>
   <Edit>Edit</Edit>   
  </label>
  <msg>
   <no-content>There is no content to show</no-content>
  </msg>
 </UIPresentationContainer>

 <UIWelcomeScreen>
  <label>
   <WelcomeLabel>Welcome to Single Content Viewer</WelcomeLabel>
   <WelcomeText>Make your choice</WelcomeText>
  </label>
  <action>
   <Abort>Abort</Abort>
   <CreateNewContent>Create new content</CreateNewContent>
   <SelectContent>Select a content</SelectContent>
  </action>
 </UIWelcomeScreen>

 <UINameWebContentForm>
  <action>
   <Save>Save</Save>
   <Abort>Abort</Abort>
  </action>
  <label>
   <name>Title</name>
   <selectTemplate>Select a template</selectTemplate>
   <option>
    <exo:siteBreadcrumb>Site Breadcrumb</exo:siteBreadcrumb>
    <exo:siteNavigationWebContent>Site Navigation</exo:siteNavigationWebContent>
    <exo:pictureOnHeadWebcontent>Picture on head layout webcontent</exo:pictureOnHeadWebcontent>
    <exo:siteSearchBox>Site search box webcontent</exo:siteSearchBox>
    <exo:webContent>Free layout webcontent</exo:webContent>
   </option>
  </label>
 </UINameWebContentForm>
 


 <UITabPane>
   <title>
    <UIContentDialogConfig>Configuration for Single content viewer</UIContentDialogConfig>
   </title>
  </UITabPane>
  
  <UIContentDialogConfig>
   <action>
    <SaveToPortletPreference>Save</SaveToPortletPreference>
   </action>
   <label>
    <ShowTitle>Show title</ShowTitle>
    <ShowPrintAction>Show print</ShowPrintAction>
   </label>
   <msg>
     <saved-successfully>Your preferences is saved</saved-successfully>
   </msg>
  </UIContentDialogConfig>
  
  <UIContentDialogFormPopupWindow>
   <title>
    <UIWelcomeScreen>Welcome to Single content viewer</UIWelcomeScreen>
    <UINameWebContentForm>Template form</UINameWebContentForm>
    <UIContentDialogForm>Web content form</UIContentDialogForm>
    <UIContentSelectorOne>Single content selector panel</UIContentSelectorOne>
   </title>
  </UIContentDialogFormPopupWindow>
  <UISingleContentViewerPortlet>
  	 <msg>
  	 		<AccessControlException>You don't have permission to access edit mode</AccessControlException>
  	 </msg>
  </UISingleContentViewerPortlet>
  
  <UISCVContextualRadioBox>
  	<label>
		<Enable>Enable</Enable>
		<Disable>Disable</Disable>
  	</label>
  </UISCVContextualRadioBox>  
  
  <UIPreferencesPopupWindows>
	<title>
		<UISCVPreferences>Content Detail Preferences</UISCVPreferences>
	</title>
  </UIPreferencesPopupWindows>
  <UISCVPreferences>
    <action>
     <Save>Save</Save>
     <Abort>Abort</Abort>
     <Cancel>Cancel</Cancel>
     <Print>Print</Print>
     <Close>Close</Close>
    </action>
    <title>Content Detail Preferences</title>
    <label>
      <Back>Back to previous page</Back>
   	  <Title>Content Detail Preferences</Title>
   	  <UISCVShowTitleConfigurationCheckBox>Show Title</UISCVShowTitleConfigurationCheckBox>
   	  <UISCVShowDateConfigurationCheckBox>Show Date</UISCVShowDateConfigurationCheckBox>
   	  <UISCVShowOptionBarConfigurationCheckBox>Show Option Bar</UISCVShowOptionBarConfigurationCheckBox>
   	  <DisplaySettingBlock>Display settings</DisplaySettingBlock>
   	  <ContentPathBlock>Content Selection</ContentPathBlock>
   	  <UISCVConfigItemPathFormInputSet>Content Path</UISCVConfigItemPathFormInputSet>
   	  <Advance>Advanced</Advance>
   	  <ContextCaption>Dynamic Navigation</ContextCaption>
   	  <UISCVContextualRadioBox>Contextual Content</UISCVContextualRadioBox>
   	  <UISCVParameterInputBox>by</UISCVParameterInputBox>
   	  <PrintSettingBlock>Print viewer setting</PrintSettingBlock>
   	  <UISCVPrintPageParameter>URL Parameter</UISCVPrintPageParameter>
   	  <UISCVConfigPrintPageFormInputSet>Print viewer in page</UISCVConfigPrintPageFormInputSet>
   	  <LockStatus>Lock</LockStatus>	
    </label>
    <help>
   	  <ContentPathBlock>Please select the source data to display</ContentPathBlock>
   	  <DisplaySettingBlock>You can determine if you want to show Title/Created date of content also the Option bar to print</DisplaySettingBlock>
   	  <ContextualHelp>This option allow you to enable/disable the contextual parameter for the shown content of portlet</ContextualHelp>
   	  <ParameterHelp>Specify the parameter which appear on the URL</ParameterHelp>
    </help>   
  </UISCVPreferences>
  <UISCVConfigItemPathFormInputSet>
  	<title>
  		<SelectFolderPath>Select folder path</SelectFolderPath>
  	</title>  	
  </UISCVConfigItemPathFormInputSet>
  <UISCVConfigPrintPageFormInputSet>
  	<title>
  		<SelectTargetPage>Print viewer in page</SelectTargetPage>
  	</title>  	
  </UISCVConfigPrintPageFormInputSet>
</bundle>
