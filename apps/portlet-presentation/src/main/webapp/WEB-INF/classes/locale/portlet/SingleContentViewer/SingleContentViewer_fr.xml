<?xml version="1.0" encoding="UTF-8"?>
<bundle>
 <UIPresentationContainer>
  <title>
   <QuickEdit>Éditer rapidement</QuickEdit>
   <print>Imprimer cette page</print>
  </title>
  <label>
   <draft>brouillon</draft>
   <print>Imprimer</print>
  </label>
  <msg>
   <no-content>Il n'ya pas de contenu à afficher</no-content>
  </msg>
 </UIPresentationContainer>

 <UIWelcomeScreen>
  <label>
   <WelcomeLabel>Bienvenue dans Single Content Viewer</WelcomeLabel>
   <WelcomeText>Faites votre choix</WelcomeText>
  </label>
  <action>
   <Abort>Annuler</Abort>
   <CreateNewContent>Créer un nouveau contenu</CreateNewContent>
   <SelectContent>Sélectionnez un contenu</SelectContent>
  </action>
 </UIWelcomeScreen>

 <UINameWebContentForm>
  <action>
   <Save>Sauver</Save>
   <Abort>Annuler</Abort>
  </action>
  <label>
   <name>Titre</name>
   <selectTemplate>Sélectionnez un modèle</selectTemplate>
   <option>
    <exo:siteBreadcrumb>Site Breadcrumb</exo:siteBreadcrumb>
    <exo:siteNavigationWebContent>Site Navigation</exo:siteNavigationWebContent>
    <exo:pictureOnHeadWebcontent>Picture on head layout webcontent</exo:pictureOnHeadWebcontent>
    <exo:siteSearchBox>Site search box webcontent</exo:siteSearchBox>
    <exo:webContent>Free layout webcontent</exo:webContent>
   </option>
  </label>
 </UINameWebContentForm>

 <UITabPane>
   <title>
    <UIContentDialogConfig>Configuration pour Single content viewer</UIContentDialogConfig>
   </title>
  </UITabPane>
  
  <UIContentDialogConfig>
   <action>
    <SaveToPortletPreference>Sauver</SaveToPortletPreference>
   </action>
   <label>
    <ShowTitle>Afficher le titre</ShowTitle>
    <ShowPrintAction>Afficher l'imprimer</ShowPrintAction>
   </label>
   <msg>
    <saved-successfully>Vos préférences sont enregistrées</saved-successfully>
   </msg>
  </UIContentDialogConfig>
  
  <UIContentDialogFormPopupWindow>
   <title>
    <UIWelcomeScreen>Bienvenue dans Single content viewer</UIWelcomeScreen>
    <UINameWebContentForm>Suivant un modèle</UINameWebContentForm>
    <UIContentDialogForm>Web content form</UIContentDialogForm>
    <UIContentSelectorOne>Single content selector panel</UIContentSelectorOne>
   </title>
  </UIContentDialogFormPopupWindow>
  
  <UISingleContentViewerPortlet>
  	 <msg>
  	 		<AccessControlException>Vous n'avez pas la permission d'accéder à modifier le mode</AccessControlException>
  	 </msg>
  </UISingleContentViewerPortlet>
  <UIPreferencesPopupWindows>
	<title>
		<UISCVPreferences>Content Detail Preferences</UISCVPreferences>
	</title>
  </UIPreferencesPopupWindows>
  <UISCVPreferences>	
    <action>
     <Save>Save</Save>
     <Abort>Abort</Abort>
     <Cancel>Cancel</Cancel>
    </action>
    <title>Content Detail Preferences</title>
    <label>
      <Edit>Edit</Edit>
      <Preferences>Preferences</Preferences>
   	  <Title>Content Detail Preferences</Title>
   	  <UISCVShowTitleConfigurationCheckBox>Show Title</UISCVShowTitleConfigurationCheckBox>
   	  <UISCVShowDateConfigurationCheckBox>Show Date</UISCVShowDateConfigurationCheckBox>
   	  <UISCVShowOptionBarConfigurationCheckBox>Show Option Bar</UISCVShowOptionBarConfigurationCheckBox>
   	  <DisplaySettingBlock>Display settings</DisplaySettingBlock>
   	  <ContentPathBlock>Content Selection</ContentPathBlock>
   	  <UISCVConfigItemPathFormInputSet>Content Path</UISCVConfigItemPathFormInputSet>
    </label>
    <help>
   	  <ContentPathBlock>Please select the source data to display</ContentPathBlock>
   	  <DisplaySettingBlock>You can determine if you want to show Title/Created date of content also the Option bar to print</DisplaySettingBlock>
    </help>   
  </UISCVPreferences>
</bundle>