    <action>
      <Save>Salva</Save>
      <Cancel>Annulla</Cancel>
    </action>
  </UIPCLVConfig>

  <OrderTypes>
    <label>
      <OrderDesc>Descrescente</OrderDesc>
      <OrderAsc>Crescente</OrderAsc>
    </label>
  </OrderTypes>

  <UIParameterizedTagetPageInputSetAction>
    <title>
      <SelectTargetPage>Seleziona Pagina di Destinazione</SelectTargetPage>
    </title>
  </UIParameterizedTagetPageInputSetAction>
