      <UICLVConfigShowTitleFormCheckboxInput>Afficher le titre</UICLVConfigShowTitleFormCheckboxInput>
      <UICLVConfigShowDateCreatedFormCheckboxInput>Afficher la date</UICLVConfigShowDateCreatedFormCheckboxInput>
      <UICLVConfigShowLinkFormCheckboxInput>Afficher le lien</UICLVConfigShowLinkFormCheckboxInput>
      <UICLVConfigShowAutomaticDetectionCheckboxInput>Détection automatique</UICLVConfigShowAutomaticDetectionCheckboxInput>      
    </label>
    <action>
      <Save>Sauver</Save>
      <Cancel>Annuler</Cancel>
    </action>
    <msg>
      <not-valid-path>Le chemin élément est invalide</not-valid-path>
      <saving-success>Les préférences du portlet sont enregistrés</saving-success>
    </msg>
  </UICLVConfig>

  <UICLVConfigDisplayModeFormRadioBoxInput>
    <label>
      <AutoViewerMode>Par Répertoire</AutoViewerMode>
      <ManualViewerMode>Par Contenus</ManualViewerMode>
    </label>
  </UICLVConfigDisplayModeFormRadioBoxInput>

  <UICLVConfigItemPathFormInputSet>
    <title>
      <SelectFolderPath>Sélectionner le dossier chemin</SelectFolderPath>
    </title>
  </UICLVConfigItemPathFormInputSet>

  <FolderPathSelectorPopupWindow>
    <title>
      <UIContentSelectorFolder>Navigateur de dossiers</UIContentSelectorFolder>
    </title>
  </FolderPathSelectorPopupWindow>

  <UICLVConfigOrderTypeFormRadioBoxInput>
    <label>
      <OrderDesc>Descendant</OrderDesc>
      <OrderAsc>Ascendant</OrderAsc>
    </label>
  </UICLVConfigOrderTypeFormRadioBoxInput>

  <UICLVConfigTargetPageFormInputSet>
    <title>
      <SelectBasePath>Sélectionnez le chemin de base</SelectBasePath>
    </title>
  </UICLVConfigTargetPageFormInputSet>

  <UICLVBasePathPopupWindow>
    <title>
      <UIPageSelector>Sélecteur de page</UIPageSelector>
    </title>
  </UICLVBasePathPopupWindow>

