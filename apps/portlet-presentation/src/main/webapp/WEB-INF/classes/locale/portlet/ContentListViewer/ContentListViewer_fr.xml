<?xml version="1.0" encoding="UTF-8"?>
<bundle>
  <UICLVContainer>
    <msg>
      <non-contents>Il n'ya pas de contenu ici</non-contents>
      <item-not-found>Cet article ne peut pas trouver</item-not-found>
