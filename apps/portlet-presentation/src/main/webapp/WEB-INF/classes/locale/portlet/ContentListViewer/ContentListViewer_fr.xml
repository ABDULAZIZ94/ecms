<?xml version="1.0" encoding="UTF-8"?>
<bundle>
  <UICLVContainer>
    <title>
      <QuickEdit>Éditer rapidement</QuickEdit>
    </title>
    <msg>
      <non-contents>Il n'ya pas de contenu ici</non-contents>
      <item-not-found>Cet article ne peut pas trouver</item-not-found>
      <no-permission>Vous n'avez pas la permission d'accéder à ce noeud</no-permission>
    </msg>
  </UICLVContainer>

  <UIViewerManagementPopupWindow>
    <title>
      <UICLVConfig>Configuration d'une liste de contenu</UICLVConfig>
    </title>
  </UIViewerManagementPopupWindow>

  <UICLVConfig>
    <label>
      <Title>Configuration d'une liste de contenu</Title>
      <ContentSelectionBlock>Sélection de contenus</ContentSelectionBlock>
      <UICLVConfigDisplayModeFormRadioBoxInput>Mode</UICLVConfigDisplayModeFormRadioBoxInput>
      <UICLVConfigItemPathFormInputSet>Chemin du dossier</UICLVConfigItemPathFormInputSet>
      <UICLVConfigOrderByFormSelectBox>Classement par</UICLVConfigOrderByFormSelectBox>
      <option>
        <exo:title>Titre</exo:title>
        <exo:dateCreated>Date de création</exo:dateCreated>
        <exo:dateModified>Date de modification</exo:dateModified>
        <publication:liveDate>Date de publication</publication:liveDate>
        <exo:startEvent>Date d'événement</exo:startEvent>
      </option>
      <DisplaySettingBlock>Réglages</DisplaySettingBlock>
      <UICLVConfigHeaderFormStringInput>Entête</UICLVConfigHeaderFormStringInput>
      <UICLVConfigDisplayTemplateFormSelectBox>Modèle de vue</UICLVConfigDisplayTemplateFormSelectBox>
      <UICLVConfigPaginatorTemplateFormSelectBox>Modèle de pagination</UICLVConfigPaginatorTemplateFormSelectBox>
      <UICLVConfigTargetPageFormInputSet>URL de destination</UICLVConfigTargetPageFormInputSet>
      <UICLVConfigTargetPageFormStringInput></UICLVConfigTargetPageFormStringInput>
      <UICLVConfigItemsPerPageFormStringInput>Contenus par page</UICLVConfigItemsPerPageFormStringInput>
      <UICLVConfigShowIllustrationFormCheckboxInput>Afficher l'illustration</UICLVConfigShowIllustrationFormCheckboxInput>
      <UICLVConfigShowSummaryFormCheckboxInput>Afficher le résumé</UICLVConfigShowSummaryFormCheckboxInput>
      <UICLVConfigShowHeaderFormCheckboxInput>Afficher l'entête</UICLVConfigShowHeaderFormCheckboxInput>
      <UICLVConfigShowRefreshFormCheckboxInput>Afficher le bouton actualiser</UICLVConfigShowRefreshFormCheckboxInput>
      <UICLVConfigShowTitleFormCheckboxInput>Afficher le titre</UICLVConfigShowTitleFormCheckboxInput>
      <UICLVConfigShowDateCreatedFormCheckboxInput>Afficher la date</UICLVConfigShowDateCreatedFormCheckboxInput>
      <UICLVConfigShowLinkFormCheckboxInput>Afficher le lien</UICLVConfigShowLinkFormCheckboxInput>
      <UICLVConfigShowReadmoreFormCheckboxInput>Lire la suite</UICLVConfigShowReadmoreFormCheckboxInput>
    </label>
    <action>
      <Save>Sauver</Save>
      <Cancel>Annuler</Cancel>
    </action>
    <msg>
      <not-valid-path>Le chemin élément est invalide</not-valid-path>
      <saving-success>Les préférences du portlet sont enregistrés</saving-success>
    </msg>
  </UICLVConfig>

  <UICLVConfigDisplayModeFormRadioBoxInput>
    <label>
      <AutoViewerMode>Par Répertoire</AutoViewerMode>
      <ManualViewerMode>Par Contenus</ManualViewerMode>
    </label>
  </UICLVConfigDisplayModeFormRadioBoxInput>

  <UICLVConfigItemPathFormInputSet>
    <title>
      <SelectFolderPath>Sélectionner le dossier chemin</SelectFolderPath>
    </title>
  </UICLVConfigItemPathFormInputSet>

  <FolderPathSelectorPopupWindow>
    <title>
      <UIContentSelectorFolder>Navigateur de dossiers</UIContentSelectorFolder>
    </title>
  </FolderPathSelectorPopupWindow>

  <UICLVConfigOrderTypeFormRadioBoxInput>
    <label>
      <OrderDesc>Descendant</OrderDesc>
      <OrderAsc>Ascendant</OrderAsc>
    </label>
  </UICLVConfigOrderTypeFormRadioBoxInput>

  <UICLVConfigTargetPageFormInputSet>
    <title>
      <SelectBasePath>Sélectionnez le chemin de base</SelectBasePath>
    </title>
  </UICLVConfigTargetPageFormInputSet>

  <UICLVBasePathPopupWindow>
    <title>
      <UIPageSelector>Sélecteur de page</UIPageSelector>
    </title>
  </UICLVBasePathPopupWindow>

  <UICLVPresentation>
    <label>
      <readmore>Lire la suite</readmore>
    </label>
    <action>
      <refresh>Rafraîchir</refresh>
      <edit>Modifier</edit>
    </action>
    <label>
      <draft>Brouillon</draft>
    </label>
  </UICLVPresentation>

  <CorrectContentSelectorPopupWindow>
    <title>
      <UIContentSelectorMulti>Multiples panneau sélecteur de contenu</UIContentSelectorMulti>
    </title>
  </CorrectContentSelectorPopupWindow>

  <UIContentDialogFormPopupWindow>
    <title>
      <UIContentDialogForm>Edition du contenu</UIContentDialogForm>
    </title>
  </UIContentDialogFormPopupWindow>

</bundle>