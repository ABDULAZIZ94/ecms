      <UICLVConfigShowLinkFormCheckboxInput>Afficher le lien</UICLVConfigShowLinkFormCheckboxInput>
      <UICLVConfigShowAutomaticDetectionCheckboxInput>Détection automatique</UICLVConfigShowAutomaticDetectionCheckboxInput>      
    </label>
    <help>
    	<ModeSelectionHelp>Determine what kind of information will be display inside portlet</ModeSelectionHelp>
    	<FolderPathHelp>Select the location of information</FolderPathHelp>
    	<HeaderOptionHelp>Set the title to present the general contents inside portlet</HeaderOptionHelp>
    	<GeneralOptionHelp>Determine if you want to display the corresponding item</GeneralOptionHelp>
    	<ContextualHelp>This option allow you to enable/disable the contextual parameter for the shown content of portlet</ContextualHelp>
       	<ParameterHelp>Specify the parameter which appear on the URL</ParameterHelp>
       	<ShowDetailInPageHelp>Specify the name of page which will be used to display the content details for an item</ShowDetailInPageHelp>    	
       	<DetailParameterHelp>Specify the parameter on URL which the detail page will use to get the content location</DetailParameterHelp>
    </help>
    <action>
      <Save>Sauver</Save>
      <Cancel>Annuler</Cancel>
    </action>
    <msg>
      <not-valid-path>Le chemin élément est invalide</not-valid-path>
      <saving-success>Les préférences du portlet sont enregistrés</saving-success>
    </msg>
  </UICLVConfig>

  <UICLVConfigDisplayModeFormRadioBoxInput>
    <label>
      <AutoViewerMode>Par Répertoire</AutoViewerMode>
      <ManualViewerMode>Par Contenus</ManualViewerMode>
    </label>
  </UICLVConfigDisplayModeFormRadioBoxInput>

  <UICLVConfigItemPathFormInputSet>
    <title>
      <SelectFolderPath>Sélectionner le dossier chemin</SelectFolderPath>
    </title>
  </UICLVConfigItemPathFormInputSet>

  <FolderPathSelectorPopupWindow>
    <title>
      <UIContentSelectorFolder>Navigateur de dossiers</UIContentSelectorFolder>
    </title>
  </FolderPathSelectorPopupWindow>

  <UICLVConfigOrderTypeFormRadioBoxInput>
    <label>
      <OrderDesc>Descendant</OrderDesc>
      <OrderAsc>Ascendant</OrderAsc>
    </label>
  </UICLVConfigOrderTypeFormRadioBoxInput>

  <UICLVConfigTargetPageFormInputSet>
    <title>
      <SelectBasePath>Sélectionnez le chemin de base</SelectBasePath>
    </title>
  </UICLVConfigTargetPageFormInputSet>

  <UICLVBasePathPopupWindow>
    <title>
