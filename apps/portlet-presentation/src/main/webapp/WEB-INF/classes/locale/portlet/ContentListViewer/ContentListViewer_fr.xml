      <UICLVConfigShowReadmoreFormCheckboxInput>Afficher "En savoir plus"</UICLVConfigShowReadmoreFormCheckboxInput>
      <UICLVConfigShowIllustrationFormCheckboxInput>Afficher l'image</UICLVConfigShowIllustrationFormCheckboxInput>
      <AdvancedBlock>Paramètres avancés</AdvancedBlock>
	  <UICLVConfigDynamicNavigationLabel>Navigation dynamique</UICLVConfigDynamicNavigationLabel>
	  <UICLVConfigContextualFolderRadioBoxInput>Répertoire contextuel</UICLVConfigContextualFolderRadioBoxInput> 	
	  <UICLVConfigShowCLVByStringInput>par</UICLVConfigShowCLVByStringInput>
	  <UICLVConfigshowSCVWithStringInput>avec</UICLVConfigshowSCVWithStringInput>
    <UICLVConfigTargetPageFormInputSet>Afficher dans une page</UICLVConfigTargetPageFormInputSet>
    <content-not-found>Ce contenu n'est pas trouvé!</content-not-found>
    </label>
    <help>
    	<ModeSelectionHelp>Configure quelle type de contenu sera affiché dans cette portlet</ModeSelectionHelp>
    	<FolderPathHelp>Selectionne le chemin où se trouve les contenus</FolderPathHelp>
    	<HeaderOptionHelp>Configure les informations générales à afficher dans la portlet</HeaderOptionHelp>
    	<GeneralOptionHelp>Configure l'affichage ou non du paramètre correspondant</GeneralOptionHelp>
    	<ContextualHelp>This option allow you to enable/disable the contextual parameter for the shown content of portlet</ContextualHelp>
       	<ParameterHelp>Specify the parameter which appear on the URL</ParameterHelp>
       	<ShowDetailInPageHelp>Specify the name of page which will be used to display the content details for an item</ShowDetailInPageHelp>    	
       	<DetailParameterHelp>Specify the parameter on URL which the detail page will use to get the content location</DetailParameterHelp>
    </help>
  </UICLVConfig>
  
  <UICLVPresentation>
    <label>
	  <rssFeed>Flux RSS</rssFeed>      
	</label>
  </UICLVPresentation>
  
  <UICLVConfigContextualFolderRadioBoxInput>
  	<label>
		  <contextualEnable>Activer</contextualEnable>
		  <contextualDisable>Désactiver</contextualDisable>
  	</label>
  </UICLVConfigContextualFolderRadioBoxInput>
    
  <PublicationStates>
    <archived>Archivé</archived>
    <obsolete>Obsolète</obsolete>
    <unpublished>Non publié</unpublished>
    <enrolled>Créé</enrolled>
    <draft>Brouillon</draft>
    <pending>Rédigé</pending>
    <approved>Validé</approved>
    <inreview>En revue</inreview>
    <staged>Prêt à publier</staged>
    <published>Publié</published>
  </PublicationStates>

