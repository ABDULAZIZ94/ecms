<?xml version="1.0" encoding="UTF-8"?>
<bundle>
  <UICLVContainer>
    <title>
      <QuickEdit>Quick edit</QuickEdit>
    </title>
    <msg>
      <non-contents>There are no content here</non-contents>
      <item-not-found>Item not found</item-not-found>
      <no-permission>You don't have permission to access this node</no-permission>
    </msg>
  </UICLVContainer>

  <UIViewerManagementPopupWindow>
    <title>
      <UICLVConfig>Content list viewer configuration</UICLVConfig>
    </title>
  </UIViewerManagementPopupWindow>

  <UICLVConfig>
    <label>
      <Title>Content list viewer configuration</Title>
      <ContentSelectionBlock>Contents selection</ContentSelectionBlock>
      <UICLVConfigDisplayModeFormRadioBoxInput>Mode</UICLVConfigDisplayModeFormRadioBoxInput>
      <UICLVConfigItemPathFormInputSet>Folder path</UICLVConfigItemPathFormInputSet>
      <UICLVConfigOrderByFormSelectBox>Order by</UICLVConfigOrderByFormSelectBox>
      <option>
        <exo:title>Title</exo:title>
        <exo:dateCreated>Date created</exo:dateCreated>
        <exo:dateModified>Date modified</exo:dateModified>
        <publication:liveDate>Date published</publication:liveDate>
      </option>
      <DisplaySettingBlock>Display settings</DisplaySettingBlock>
      <UICLVConfigHeaderFormStringInput>Header</UICLVConfigHeaderFormStringInput>
      <UICLVConfigDisplayTemplateFormSelectBox>Viewer template</UICLVConfigDisplayTemplateFormSelectBox>
      <UICLVConfigPaginatorTemplateFormSelectBox>Paginator template</UICLVConfigPaginatorTemplateFormSelectBox>
      <UICLVConfigTargetPageFormInputSet>Base path</UICLVConfigTargetPageFormInputSet>
      <UICLVConfigTargetPageFormStringInput></UICLVConfigTargetPageFormStringInput>
      <UICLVConfigItemsPerPageFormStringInput>Items per page</UICLVConfigItemsPerPageFormStringInput>
      <UICLVConfigShowIllustrationFormCheckboxInput>Show illustration</UICLVConfigShowIllustrationFormCheckboxInput>
      <UICLVConfigShowSummaryFormCheckboxInput>Show summary</UICLVConfigShowSummaryFormCheckboxInput>
      <UICLVConfigShowHeaderFormCheckboxInput>Show header</UICLVConfigShowHeaderFormCheckboxInput>
      <UICLVConfigShowRefreshFormCheckboxInput>Show refresh button</UICLVConfigShowRefreshFormCheckboxInput>
      <UICLVConfigShowTitleFormCheckboxInput>Show title</UICLVConfigShowTitleFormCheckboxInput>
      <UICLVConfigShowDateCreatedFormCheckboxInput>Show date created</UICLVConfigShowDateCreatedFormCheckboxInput>
      <UICLVConfigShowLinkFormCheckboxInput>Show link</UICLVConfigShowLinkFormCheckboxInput>
      <UICLVConfigShowReadmoreFormCheckboxInput>Read more</UICLVConfigShowReadmoreFormCheckboxInput>
    </label>
    <action>
      <Save>Save</Save>
      <Cancel>Cancel</Cancel>
    </action>
    <msg>
      <not-valid-path>The item path is invalid</not-valid-path>
      <saving-success>The portlet's preferences are saved</saving-success>
    </msg>
  </UICLVConfig>

  <UICLVConfigDisplayModeFormRadioBoxInput>
    <label>
      <AutoViewerMode>By Folder</AutoViewerMode>
      <ManualViewerMode>By Contents</ManualViewerMode>
    </label>
  </UICLVConfigDisplayModeFormRadioBoxInput>

  <UICLVConfigItemPathFormInputSet>
    <title>
      <SelectFolderPath>Select folder path</SelectFolderPath>
    </title>
  </UICLVConfigItemPathFormInputSet>

  <FolderPathSelectorPopupWindow>
    <title>
      <UIContentSelectorFolder>Folder browser</UIContentSelectorFolder>
    </title>
  </FolderPathSelectorPopupWindow>

  <UICLVConfigOrderTypeFormRadioBoxInput>
    <label>
      <OrderDesc>Descendant</OrderDesc>
      <OrderAsc>Ascendant</OrderAsc>
    </label>
  </UICLVConfigOrderTypeFormRadioBoxInput>

  <UICLVConfigTargetPageFormInputSet>
    <title>
      <SelectBasePath>Select base path</SelectBasePath>
    </title>
  </UICLVConfigTargetPageFormInputSet>

  <UICLVBasePathPopupWindow>
    <title>
      <UIPageSelector>Page selector</UIPageSelector>
    </title>
  </UICLVBasePathPopupWindow>

  <UICLVPresentation>
    <label>
      <readmore>Read more</readmore>
    </label>
    <action>
      <refresh>Refresh</refresh>
      <edit>Edit</edit>
    </action>
    <label>
      <draft>Draft</draft>
    </label>
  </UICLVPresentation>

  <CorrectContentSelectorPopupWindow>
    <title>
      <UIContentSelectorMulti>Multiple content selector panel</UIContentSelectorMulti>
    </title>
  </CorrectContentSelectorPopupWindow>

  <UIContentDialogFormPopupWindow>
    <title>
      <UIContentDialogForm>Web content form</UIContentDialogForm>
    </title>
  </UIContentDialogFormPopupWindow>
  
</bundle>