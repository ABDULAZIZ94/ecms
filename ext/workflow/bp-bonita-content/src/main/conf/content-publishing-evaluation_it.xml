<?xml version="1.0" encoding="UTF-8"?>
<bundle>
  <task-name>Valuta Documento</task-name>
  <title>Valuta Documento</title>
  <decision>
    <label>Decision:</label>
    <radiobox-0>Approva Documento</radiobox-0>
    <radiobox-1>Disapprova Documento</radiobox-1>
    <radiobox-2>Delega Validazione</radiobox-2>
    <radiobox-3>Rifiuta Documento</radiobox-3>
  </decision>
  <approve>
    <submit>Approva</submit>
    <value>Approva Documento</value>
  </approve>
  <disapprove>
    <submit>Disapprova</submit>
    <value>Disapprova Documento</value>
