    <label>Publicazione inizia il:</label>
  </startDate>
  <endDate>
    <title>Quando interrompere la pubblicazione</title>
    <label>Publicazione finisce il:</label>
  </endDate>
  <delegator>
    <title>Delega Validazione</title>
    <label>Delega Validazione:</label>
  </delegator>
  <documento-id>
    <title>Documento</title>
    <label>Documento:</label>
  </documento-id>
  <reason>
    <label>Ragione:</label>
    <title>Ragione</title>
  </reason>
